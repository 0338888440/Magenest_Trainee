<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left"
      xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <head>
        <script src="Magenest_KnockoutJs::js/view/savecustomer.js"/>
    </head>
    <body>
        <referenceContainer name="content">
            <block class="Magenest\KnockoutJs\Block\Search" name="knockout_test_search" template="search.phtml" cacheable="false">
                <arguments>
                    <argument name="jsLayout" xsi:type="array">
                        <item name="components" xsi:type="array">
                            <item name="search" xsi:type="array">
                                <item name="component" xsi:type="string">Magenest_KnockoutJs/js/view/search</item>
                                <item name="displayArea" xsi:type="string">search</item>
                            </item>
                        </item>
                    </argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>

</page>